<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Sample Category"
            id="DebuggerPlugin.commands.category">
      </category>
      <command
            name="Sample Command"
            categoryId="DebuggerPlugin.commands.category"
            id="DebuggerPlugin.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="DebuggerPlugin.commands.sampleCommand"
            class="debuggerplugin.handlers.SampleHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="DebuggerPlugin.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Sample Menu"
               mnemonic="M"
               id="DebuggerPlugin.menus.sampleMenu">
            <command
                  commandId="DebuggerPlugin.commands.sampleCommand"
                  mnemonic="S"
                  id="DebuggerPlugin.menus.sampleCommand">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="DebuggerPlugin.toolbars.sampleToolbar">
            <command
                  commandId="DebuggerPlugin.commands.sampleCommand"
                  icon="icons/sample.gif"
                  tooltip="Say hello world"
                  id="DebuggerPlugin.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <category
            name="Sample File Import"
            id="debuggerplugin.importWizards.sampleCategory">
      </category>
      <wizard
            name="Import File"
            icon="icons/sample.gif"
            category="debuggerplugin.importWizards.sampleCategory"
            class="debuggerplugin.importWizards.ImportWizard"
            id="debuggerplugin.importWizards.ImportWizard">
         <description>
            Import a file from the local file system into the workspace.
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Sample Multi-page Editor"
            extensions="mpe"
            icon="icons/sample.gif"
            contributorClass="debuggerplugin.editors.MultiPageEditorContributor"
            class="debuggerplugin.editors.MultiPageEditor"
            id="debuggerplugin.editors.MultiPageEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="Sample Wizards"
            id="DebuggerPlugin">
      </category>
      <wizard
            name="Multi-page Editor file"
            icon="icons/sample.gif"
            category="DebuggerPlugin"
            class="stanford.infolab.debugger.plugin.wizards.SampleNewWizard"
            id="debuggerplugin.wizards.SampleNewWizard">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            id="DebuggerPlugin.contribution1">
         <menu
               label="New Submenu"
               path="additions"
               id="DebuggerPlugin.menu1">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               label="New Action"
               class="debuggerplugin.popup.actions.NewAction"
               menubarPath="DebuggerPlugin.menu1/group1"
               enablesFor="1"
               id="DebuggerPlugin.newAction">
         </action>
      </objectContribution>
   </extension>
   <extension point="org.eclipse.ui.menus">
    <menuContribution locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
      <command commandId="stanford.infolab.debugger.plugin.generateUnitTest"
        label="Generate Unit Test" style="push">
        <visibleWhen checkEnabled="false">
          <with variable="activeMenuSelection">
            <iterate ifEmpty="false" operator="or">
              <adapt type="org.eclipse.jdt.core.ICompilationUnit">
              </adapt>
            </iterate>
          </with>
        </visibleWhen>
      </command>
    </menuContribution>
  </extension>
  <extension point="org.eclipse.ui.commands">
    <command defaultHandler="stanford.infolab.debugger.plugin.handler.GenerateUnitTestHandler"
      id="stanford.infolab.debugger.plugin.generateUnitTest" name="Generate Unit Test">
    </command>
  </extension>

</plugin>
